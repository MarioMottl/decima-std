builtin_list_csv :: "print,len,now_ns,array,array_with,push,pop,get,set,read_file,split,parse_int,trim"

help_builtins :: () {
    print("builtins:")
    parts := split(builtin_list_csv, ",")
    i := 0
    for (i = 0; i < len(parts); i = i + 1) {
        print(get(parts, i))
    }
}

assert :: (cond: bool, msg: string) {
    if (cond) { return }
    print("assert failed:")
    print(msg)
}

println :: (s: string) {
    print(s)
}

