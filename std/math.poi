PI :: 3.141592653589793

dot :: (a: Vec2, b: Vec2) -> int {
    return (a.x * b.x) + (a.y * b.y)
}

clamp :: (x:int, lo:int, hi:int) -> int {
    if (x < lo) { return lo }
    if (x > hi) { return hi }
    return x
}

factorial :: (n:int) -> int {
    acc: int = 1
    i: int = 2
    for (i = 2; i <= n; i += 1) {
        acc = acc * i
    }
    return acc
}

square :: (x:int)->int { return x * x }
